generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model estoque {
  id_do_tecido       Int     @id @default(autoincrement())
  nome_do_tecido     String
  valor              Float
  fornecedor         String
  composicao         String?
  largura            Float?
  peso               Float?
  estoque            Int
  data_              DateTime
  notas              String?
}
model Estoque_Agulhas {
  id_da_agulha       Int     @id @default(autoincrement())
  valor              Float
  fornecedor         String?
  numeracao          String?
  estoque            Int
  data               DateTime
  notas              String?
}
model Funcionarios {
  id                  Int     @id @default(autoincrement())
  nome_do_funcionario String
  idade               String
  funcoes             String
  data                DateTime
  notas               String?
}

